type User {
  id: Int!
  uuid: String!
  mail: String
  username: String
  status: Boolean
}

type UserConnection {
  total: Int!
  items: [User!]
}

input UserInput {
  username: String!
  mail: String!
  pass: String!
  zotero: String
  status: Boolean
}

input UserUpdateInput {
  id: Int!
  username: String
  mail: String
  pass: String
  zotero: String
  status: Boolean
}

input UserDeleteInput {
  id: Int!
}