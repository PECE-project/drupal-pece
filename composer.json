        "phpspec/prophecy-phpunit": "^2.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/console-extend-plugin": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "oomphinc/composer-installers-extender": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpro/grumphp": true,
            "phpstan/extension-installer": true,
            "php-http/discovery": true,
            "tbachert/spi": true
        }
    },
    "scripts": {
        "install-pece": [
            "composer install",
            "drush -y si pece --existing-config"
        ]
    },
    "autoload": {
        "psr-4": {
            "Drupal\\Tests\\pece_annotations\\": "web/profiles/pece/modules/pece_annotations/tests/"
        }
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/jquery-ui-touch-punch": [
                "bower-asset/jqueryui-touch-punch"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/profiles/custom/{$name}": ["type:drupal-custom-profile"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "drupal-libraries": {
            "library-directory": "web/libraries",
            "libraries": [
                {
                    "name": "objectfitpolyfill",
                    "package": "bower-asset/objectfitpolyfill"
                },
                {
                    "name": "chartjs",
                    "package": "bower-asset/chartjs"
                },
                {
                    "name": "c3",
                    "package": "bower-asset/c3"
                },
                {
                    "name": "d3",
                    "package": "bower-asset/d3"
                },
                {
                    "name": "dropzone",
                    "package": "npm-asset/dropzone"
                },
                {
                    "name": "blazy",
                    "package": "npm-asset/blazy"
                },
                {
                    "name": "slick",
                    "package": "npm-asset/slick-carousel"
                },
                {
                    "name": "splide",
                    "package": "npm-asset/splide"
                },
                {
                    "name": "ace",
                    "package": "npm-asset/ace-builds"
                },
                {
                    "name": "jquery-ui-touch-punch",
                    "package": "bower-asset/jqueryui-touch-punch"
                },
                {
                    "name": "imagesloaded",
                    "package": "npm-asset/imagesloaded"
                },
                {
                    "name": "swagger-ui",
                    "package": "npm-asset/swagger-ui-dist"
                },
                {
                    "name": "spectrum-colorpicker",
                    "package": "npm-asset/spectrum"
                }
            ]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            },
            "allowed-packages": [
                "drupal/core",
                "drupal/ui_patterns_layout_builder"
            ],
            "file-mapping": {
                "[web-root]/profiles/README.txt": false,
                "[web-root]/robots.txt": false,
                "[web-root]/profiles/.gitignore": false
            },
            "locations": {
                "web-root": "./web"
            }
        },
        "patches": {
            "drupal/core": {
                "Missing required data for configuration: x": "https://www.drupal.org/files/issues/2023-04-06/3304778-25.patch",
                "MenuLinkContent menu_name max length is too long": "https://www.drupal.org/files/issues/2024-02-23/3087632-48.patch",
                "More specific titles for term and vocabulary edit forms": "https://git.drupalcode.org/project/drupal/-/merge_requests/3883.diff",
                "Provide single field values from multivalue fields as layout builder-friendly blocks": "https://git.drupalcode.org/project/drupal/-/merge_requests/9223.diff",
                "EntityViewBuilder::getBuildDefaults doesn't respect the EntityInterface of the $entity parameter": "https://www.drupal.org/files/issues/2019-01-10/2951487_15_no-tests.patch",
                "Optimize render cache [#2888838]": "https://www.drupal.org/files/issues/2024-09-09/optimise-render-cache-2888838-61.patch",
                "Can only intentionally re-render an entity with references 20 times [#2940605]": "https://www.drupal.org/files/issues/2025-01-15/entity_render_recursion_detection_MR_8334-2940605-188.patch"
            },
            "drupal/digital_size_formatter": {
                "Support the bigint module for Big Integers [#3485326]": "https://git.drupalcode.org/project/digital_size_formatter/-/merge_requests/3.diff"
            },
            "drupal/eck": {
                "2815453 - Add migrations for Drupal 7 ECK to Drupal 8/9": "https://www.drupal.org/files/issues/2020-11-13/eck-migrate-d9-2815453-36.patch"
            },
            "drupal/facets": {
                "Links widget uses checkbox widget library #3447859": "https://git.drupalcode.org/project/facets/-/merge_requests/209.diff"
            },
            "drupal/geocoder": {
                "Have a text field that is not meant to use geocoder but cannot save widget due to geocoder_field error [#3457364]": "https://www.drupal.org/files/issues/2024-09-04/geocoder-readonly-workaround-3457364-7.patch"
            },
            "drupal/view_mode_page": {
                "Context exception when trying to create url": "https://www.drupal.org/files/issues/2025-01-29/view_mode_page-context_exception_when_creating_url-3300706-37.patch"
            }
        }
    }
}
